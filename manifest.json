{
  "manifest_version": 3,
  "name": "Internet Speed Test",
  "version": "1.0",
  "description": "Test your internet connection speed",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.jpg",
      "48": "icons/icon48.jpg",
      "128": "icons/icon128.jpg"
    }
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon16.jpg",
    "48": "icons/icon48.jpg",
    "128": "icons/icon128.jpg"
  },
  "background": {
    "service_worker": "background.js"
  }
}